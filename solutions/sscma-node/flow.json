{
    "device": {
        "id": "recamera-001",
        "name": "ReCamera"
    },
    "nodes": [
        {
            "id": "camera0",
            "type": "camera",
            "config": {
                "option": "1080p",
                "preview": true,
                "jpeg_resolution": {
                    "width": 1920,
                    "height": 1080,
                    "fps": 30
                },
                "raw_resolution": {
                    "width": 1920,
                    "height": 1080,
                    "fps": 15
                },
                "attach_channel": "raw"
            }
        },
        {
            "id": "preprocessor0",
            "type": "image_preprocessor",
            "config": {
                "width": 640,
                "height": 640,
                "debug": false,
                "enable_resize": true,
                "enable_denoising": false,
                "enable_ccw_rotation": false,
                "save_raw": true,
                "attach_channel": "raw"
            },
            "deps": [
                "camera0"
            ]
        }
    ],
    "flash_config": {
        "flash_enabled": false,
        "flash_intensity": 255,
        "flash_duration_ms": 10,
        "pre_capture_delay_ms": 50,
        "disable_red_led_blinking": true
    },
    "crop_config": {
        "enabled": false,
        "xmin": 0,
        "ymin": 375,
        "xmax": 1470,
        "ymax": 674
    },
    "white_balance_config": {
        "red_balance_factor": 0.8,
        "green_balance_factor": 0.8,
        "blue_balance_factor": 0.6
    },
    "ai_config": {
        "enabled": true,
        "model_path": "/userdata/MODEL/model.cvimodel",
        "model_lables_path": "/userdata/MODEL/model.json",
        "threshold": 0.5
    }
}